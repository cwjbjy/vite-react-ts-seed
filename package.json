{
  "name": "vite-react-ts-seed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:wc1": "node scripts/platform.js --platform=wc1 && vite --mode development -- platform=wc1",
    "dev:wc2": "node scripts/platform.js --platform=wc2 && vite --mode development -- platform=wc2",
    "dev:bash:wc1": "node shell/platform.cjs --platform=wc1 && vite --mode development -- platform=wc1",
    "dev:push": "node shell/push.cjs",
    "build": "tsc && vite build",
    "build:wc1": "node scripts/platform.js --platform=wc1 && vite build --mode production -- platform=wc1",
    "build:wc2": "node scripts/platform.js --platform=wc2 && vite build --mode production -- platform=wc2",
    "ts": "tsc --noEmit",
    "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.11.5",
    "axios": "^1.6.2",
    "immer": "^10.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "shelljs": "^0.8.5",
    "styled-components": "^6.1.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/node": "^20.10.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "inquirer": "^8.0.0",
    "lint-staged": "^15.2.0",
    "minimist": "^1.2.8",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "yarn run lint",
      "prettier --write"
    ]
  }
}
